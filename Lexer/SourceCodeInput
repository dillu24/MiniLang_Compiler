def funcSquare (x:real) : real {
	return x*x ;
}
def funcGreaterThan(x:real , y:real ) : bool {
	var ans : bool = true ;
	if ( y > x ) { set ans = false ; }
	return ans ;
}
var x : real = 2.4;
print funcGreaterThan(x,funcSquare(2.0));

