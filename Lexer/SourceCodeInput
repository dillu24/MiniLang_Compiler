def funcPow ( x : real , n : int ) : real{
	var y : real = 1.0 ;
	if (n>0){
		while ( n>0){
			set y = y * x ;
			set n = n - 1 ;
		}
	}
	else{
		while (n<0){
			set y = y / x ;
			set n = n + 1 ;
		}
	}
	return y ;
}

def hello() :string{
    return "hello";
}

print funcPow(2.0,-2);
print hello();